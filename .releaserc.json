{
  "branches": ["master"],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "releaseRules": [
          { "type": "release", "release": "minor" }
        ],
        "parserOpts": {
          "noteKeywords": ["BREAKING CHANGE"]
        }
      }
    ],
    "@semantic-release/release-notes-generator",
    "@semantic-release/github"
  ]
}
